{
  "languageserver": {
    "svlangserver": {
      "filetypes": ["verilog","systemverilog"],
      "settings": {
        "systemverilog.linter": "icarus",
        "systemverilog.launchConfiguration": "iverilog -g2012 -t null"
      }
    }
  },
  "snippets.userSnippetsDirectory": "$VIMFILES/ultisnips",
  "colors.filetypes": ["vim","lua","css"],
  "suggest.noselect":true,
  "suggest.formatItems": ["abbr", "menu", "kind"]
}
